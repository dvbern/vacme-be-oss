              "projects/vacme-initialreg/src/favicon.ico",
              {
                "glob": "**/*",
                "input": "./projects/vacme-web-shared/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "./projects/vacme-web-shared/src/styles/styleguide-be/img",
                "output": "img"
              }
            ],
